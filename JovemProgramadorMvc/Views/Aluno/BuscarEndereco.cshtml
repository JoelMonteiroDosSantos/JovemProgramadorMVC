@model EnderecoModel 

<div class="text-left">

    <div>
        <h1>Informações do CEP</h1>
    </div>

    <form>
        <div class="mb-3">
            <label for="exampleInputLogradouro1" class="form-label">Logradouro</label>
            <input type="text" class="form-control col-5" asp-for="logradouro" />
        </div>
        <div class="mb-3">
            <label for="exampleInputComplemento1" class="form-label">Complemento</label>
            <input type="text" class="form-control col-5" asp-for="complemento" />
        </div>
        <div class="mb-3">
            <label for="exampleInputBairro1" class="form-label">Bairro</label>
            <input type="text" class="form-control col-3" asp-for="bairro" />
        </div>
        <div class="mb-3">
            <label for="exampleInputLocalidade1" class="form-label">Localidade</label>
            <input type="text" class="form-control col-4" asp-for="localidade" />
        </div>
        <div class="mb-3">
            <label for="exampleInputUf1" class="form-label">Uf</label>
            <input type="text" class="form-control col-1" asp-for="uf" />
        </div>
        <div class="mb-3">
            <label for="exampleInputDdd1" class="form-label">DDD</label>
            <input type="text" class="form-control col-1" asp-for="ddd" />
        </div>
        <div class="text-center">
            <a class="btn btn-primary" type="button" asp-controller="Aluno" asp-action="Index">Voltar</a>
        </div>
    </form>
</div>